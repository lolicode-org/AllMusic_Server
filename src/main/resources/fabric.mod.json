{
  "schemaVersion": 1,
  "id": "allmusic",
  "version": "${version}",
  "name": "Allmusic",
  "description": "A server-side music player for Minecraft",
  "authors": [
    "KoishiMoe"
  ],
  "contact": {
    "repo": "https://github.com/lolicode-org/AllMusic"
  },
  "license": "MIT",
  "icon": "assets/allmusic/icon.png",
  "environment": "server",
  "entrypoints": {
    "server": [
      "org.lolicode.allmusic.Allmusic"
    ]
  },
  "mixins": [
    "allmusic.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.13",
    "fabric": "*",
    "minecraft": ">=1.19",
    "fabric-permissions-api-v0": "*",
    "fabric-language-kotlin": "*"
  }
}
